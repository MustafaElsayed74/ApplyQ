{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "projects": {
        "ai-resume-cover-letter": {
            "projectType": "application",
            "root": "",
            "sourceRoot": "src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/ai-resume-cover-letter",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": [],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "css",
                        "assets": [
                            "src/favicon.ico",
                            "src/assets"
                        ],
                        "styles": [
                            "src/styles.css""vendorChunk": false,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.ts"
                                }
                            ]
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "sourceMap": true,
                            "extractLicenses": false,
                            "namedChunks": true,
                            "vendorChunk": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.development.ts"
                                }
                            ]
                        "scripts": []
                        },
                        "defaultConfiguration": "development""configurations": {
                            "production": {
                                "buildOptimizer": true,
                                "optimization": true,
                                "outputHashing": "all",
                                "sourceMap": false,
                                "extractLicenses": true,
                                "namedChunks": false,
                                "vendorChunk": false
                            }
                        }
                    },
                    "serve": {
                        "builder": "@angular-devkit/build-angular:dev-server",
                        "options": {
                            "browserTarget": "ai-resume-cover-letter:build"
                        },
                        "configurations": {
                            "production": {
                                "browserTarget": "ai-resume-cover-letter:build:production"
                            }
                        }
                    }
                }
            }
        },
        "cli": {
            "analytics": false
        }
    }